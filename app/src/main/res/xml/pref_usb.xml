<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <de.jlab.cardroid.usb.UsbBandwidthPreference
        android:defaultValue="false"
        android:key="usb_stats_bandwidth"
        android:summary="@string/pref_description_usb_bandwidth"
        android:title="@string/pref_title_usb_bandwidth" />

    <de.jlab.cardroid.usb.UsbPacketPreference
        android:defaultValue="false"
        android:key="usb_stats_packets"
        android:summary="@string/pref_description_usb_packets"
        android:title="@string/pref_title_usb_packets" />

    <ListPreference
        android:defaultValue="115200"
        android:key="usb_baud_rate"
        android:title="@string/pref_title_usb_baud_rate"
        android:entries="@array/usb_baud_rates"
        android:entryValues="@array/usb_baud_rates"
        />

    <Preference
        android:key="car_remotes"
        android:title="@string/pref_title_remotes"
        android:summary="@string/pref_description_remotes">
        <intent android:targetPackage="de.jlab.cardroid"
            android:targetClass="de.jlab.cardroid.remotecontrol.RemoteControlManagerActivity"/>
    </Preference>

    <Preference
        android:key="car_can_sniffer"
        android:title="@string/pref_title_can_sniffer"
        android:summary="@string/pref_description_can_sniffer">
        <intent android:targetPackage="de.jlab.cardroid"
            android:targetClass="de.jlab.cardroid.cansniffer.CanSnifferActivity"/>
    </Preference>

</PreferenceScreen>
